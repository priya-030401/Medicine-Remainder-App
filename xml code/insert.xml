<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="center"
    tools:context=".insert">

    <TextView
        android:id="@+id/textView09"
        android:layout_width="232dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="610dp"
        android:background="#C6DEE1"
        android:text="Blood Pressure"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="66dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="299dp"
        android:layout_marginBottom="442dp"
        app:srcCompat="@drawable/pill1" />

    <TextView
        android:id="@+id/textView0"
        android:layout_width="166dp"
        android:layout_height="33dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="539dp"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:text="Select Medicine" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="301dp"
        android:layout_marginBottom="342dp"
        app:srcCompat="@drawable/calender" />

    <EditText
        android:id="@+id/textView012"
        android:layout_width="231dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="348dp"
        android:hint="dd/mm/yyyy"
        android:inputType="date"
        android:textSize="20sp" />

    <Spinner
        android:id="@+id/spinner02"
        android:layout_width="231dp"
        android:layout_height="54dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="192dp"
        android:entries="@array/timeofday" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="297dp"
        android:layout_marginBottom="181dp"
        app:srcCompat="@drawable/alarm2" />

    <Button
        android:id="@+id/button07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="91dp"
        android:background="#FAD8E3"
        android:text="Add^"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="217dp"
        android:layout_marginBottom="91dp"
        android:background="#FAD8E3"
        android:text="Back"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Spinner
        android:id="@+id/spinnerbp"
        android:layout_width="231dp"
        android:layout_height="54dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="452dp"
        android:entries="@array/Bloodpressure" />

    <TextView
        android:id="@+id/footer1"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:background="#F33478" />

    <Button
        android:id="@+id/homebtn1"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="330dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/ic_home"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/viewbtn1"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="222dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/ic_eye"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/setbtn1"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="109dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/ic_settings"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/profilebtn1"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/ic_profile"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="259dp"
        app:srcCompat="@drawable/ic_timer" />

    <EditText
        android:id="@+id/timer1"
        android:layout_width="68dp"
        android:layout_height="57dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="199dp"
        android:layout_marginBottom="263dp"
        android:ems="10"
        android:hint="HH:MM:SS"
        android:inputType="textPersonName|time" />

    <EditText
        android:id="@+id/min1"
        android:layout_width="89dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="267dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

</RelativeLayout>
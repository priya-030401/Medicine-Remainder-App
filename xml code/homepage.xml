<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homepage">


    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-13dp"
        android:layout_marginBottom="592dp"
        android:background="#C6DEE1"
        android:textColor="#0B0B0B"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="58dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="94dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="601dp"
        app:srcCompat="@drawable/logo1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="227dp"
        android:layout_marginBottom="605dp"
        android:text="Your Medicine Remainder"
        android:textColor="#070707"
        android:textSize="24sp"
        android:textStyle="bold" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="313dp"
        android:layout_height="238dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="52dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="336dp"
        app:srcCompat="@drawable/homepage" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="356dp"
        android:layout_height="94dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="242dp"
        android:background="#CFDDCF"
        android:textColor="#0E0210"
        android:textSize="24sp" />


    <TextView
        android:id="@+id/textView13"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="233dp"
        android:layout_marginBottom="301dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Remainder"
        android:textAlignment="center"
        android:textColor="#0E0E0E"
        android:textSize="24sp" />


    <TextView
        android:id="@+id/textView14"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="174dp"
        android:layout_marginBottom="267dp"
        android:fontFamily="sans-serif-medium"
        android:text="Next Medicine in "
        android:textColor="#090909"
        android:textSize="20sp" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="33dp"
        android:layout_height="29dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="198dp"
        android:layout_marginBottom="302dp"
        app:srcCompat="@drawable/alarm2" />

    <Button
        android:id="@+id/selectbtn"
        android:layout_width="346dp"
        android:layout_height="72dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="121dp"
        android:background="#FAD8E3"
        android:fontFamily="sans-serif-medium"
        android:text="Select Medicine Category"
        android:textColor="#090909"
        android:textSize="23sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="37dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="263dp"
        android:text="10:00 Am"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/footer"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:background="#F33478" />

    <Button
        android:id="@+id/homebtn"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="330dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/ic_home"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/viewbtn"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="222dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/ic_eye"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/setbtn"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="109dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/ic_settings"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/profilebtn"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/ic_profile"
        tools:ignore="SpeakableTextPresentCheck" />


</RelativeLayout>
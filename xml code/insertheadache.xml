<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="center"
    tools:context=".insertheadache">

    <TextView
        android:id="@+id/texthe"
        android:layout_width="232dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="613dp"
        android:background="#C6DEE1"
        android:text="         Headache"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="66dp" />

    <ImageView
        android:id="@+id/imagehe"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="289dp"
        android:layout_marginBottom="445dp"
        app:srcCompat="@drawable/pill1" />

    <TextView
        android:id="@+id/texthe1"
        android:layout_width="166dp"
        android:layout_height="33dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="555dp"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:text="Select Medicine" />

    <ImageView
        android:id="@+id/imagehe2"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="292dp"
        android:layout_marginBottom="360dp"
        app:srcCompat="@drawable/calender" />

    <EditText
        android:id="@+id/date5"
        android:layout_width="253dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="283dp"
        android:hint="dd/mm/yyyy"
        android:inputType="date"
        android:textSize="20sp" />

    <Spinner
        android:id="@+id/spinner44"
        android:layout_width="231dp"
        android:layout_height="54dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="192dp"
        android:entries="@array/timeofday" />

    <ImageView
        android:id="@+id/imagehe3"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="297dp"
        android:layout_marginBottom="181dp"
        app:srcCompat="@drawable/alarm2" />

    <Button
        android:id="@+id/btnadd4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="106dp"
        android:background="#FAD8E3"
        android:text="Add^"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnback4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="219dp"
        android:layout_marginBottom="106dp"
        android:background="#FAD8E3"
        android:text="Back"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/timerimg6"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="298dp"
        android:layout_marginBottom="273dp"
        app:srcCompat="@drawable/ic_timer" />

    <EditText
        android:id="@+id/timer6"
        android:layout_width="232dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="364dp"
        android:ems="10"
        android:hint="HH:MM:SS"
        android:inputType="textPersonName|time" />

    <EditText
        android:id="@+id/medi5"
        android:layout_width="254dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="456dp"
        android:ems="10"
        android:hint="Enter Medicine"
        android:inputType="textPersonName"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/footer6"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:background="#F33478" />

    <Button
        android:id="@+id/homebtn6"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="330dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/ic_home"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/viewbtn6"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="222dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/ic_eye"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/setbtn6"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="109dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/ic_settings"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/profilebtn6"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/ic_profile"
        tools:ignore="SpeakableTextPresentCheck" />
</RelativeLayout>